{
  "kind": "collectionType",
  "collectionName": "event_registrations",
  "info": {
    "singularName": "event-registration",
    "pluralName": "event-registrations",
    "displayName": "EventRegistration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FullName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100,
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "Phone": {
      "type": "string",
      "minLength": 9,
      "maxLength": 10,
      "required": true,
      "unique": true
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event"
    }
  }
}
